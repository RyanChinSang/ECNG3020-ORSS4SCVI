<!--TODO: Rename these titles-->

<preference-headers xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- These settings headers are only used on tablets. -->

    <header
        android:fragment="org.tensorflow.demo.SettingsActivity$SettingsPreferenceFragment"
        android:icon="@drawable/ic_settings_black_24dp"
        android:title="@string/pref_header_settings" />

    <header
        android:id="@+id/about"
        android:icon="@drawable/ic_info_black_24dp"
        android:title="@string/pref_header_about">
        <intent android:targetPackage="org.tensorflow.demo"
                android:targetClass="org.tensorflow.demo.AboutActivity"/>
    </header>


    <header
        android:id="@+id/exit_menu"
        android:icon="@drawable/ic_power_settings_new_black_24dp"
        android:title="@string/pref_header_exit" />

</preference-headers>
